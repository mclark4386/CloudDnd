<header>
  <%= link_to logo, root_path %>
  <nav class="round">
    <ul>
      <li><%= link_to "Home", root_path %></li>
	<% user = @current_user %>
	<%= user %>
	<% if user %>
            <li><%= link_to "Logout", logout_path %></li>
	<% else %>
            <li><%= link_to "Sign Up", signup_path %></li>
            <li><%= link_to "Login", login_path %></li>
	<% end %>
    </ul>
  </nav>
</header>
